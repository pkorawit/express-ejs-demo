<% include ../partials/header %>
<div class="form-group">
  <label for="">ID</label>
  <input type="text"
    class="form-control" name="" id="" aria-describedby="helpId" placeholder="" value="<%= product.id %>">
  <small id="helpId" class="form-text text-muted">Help text</small>
  <label for="">Title</label>
  <input type="text"
    class="form-control" name="" id="" aria-describedby="helpId" placeholder="" value="<%= product.title %>">
  <small id="helpId" class="form-text text-muted">Help text</small>  
  <label for="">Prize</label>
  <input type="text"
    class="form-control" name="" id="" aria-describedby="helpId" placeholder="" value="<%= product.price %>">
  <small id="helpId" class="form-text text-muted">Help text</small>
  <label for="">Create At</label>
  <input type="datetime"
    class="form-control" name="" id="" aria-describedby="helpId" placeholder="" value="<%= product.created_at %>">
  <small id="helpId" class="form-text text-muted">Help text</small>
  <button type="submit" class="btn btn-primary">Save</button>
</div>
<% include ../partials/footer %>